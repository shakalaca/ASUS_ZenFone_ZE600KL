# servicemanager - the Binder context manager
type servicemanager, domain;
type servicemanager_exec, exec_type, file_type;

init_daemon_domain(servicemanager)

# Note that we do not use the binder_* macros here.
# servicemanager is unique in that it only provides
# name service (aka context manager) for Binder.
# As such, it only ever receives and transfers other references
# created by other domains.  It never passes its own references
# or initiates a Binder IPC.
allow servicemanager self:binder set_context_mgr;
allow servicemanager domain:binder transfer;

# Check SELinux permissions.
selinux_check_access(servicemanager)

#for modem crash log ramdump
allow servicemanager subsystem_ramdump:dir { search };
allow servicemanager subsystem_ramdump:file { read open };
allow servicemanager subsystem_ramdump:process { getattr };
allow servicemanager init_shell:dir { search };
allow servicemanager init_shell:file { read open };
allow servicemanager init_shell:process { getattr };

allow servicemanager init:dir search ;
allow servicemanager init:file {read open };
allow servicemanager init:process getattr ;

userdebug_or_eng(`
	allow servicemanager WifiTest_Agent:dir { search read };
	allow servicemanager WifiTest_Agent:file rw_file_perms;
	allow servicemanager WifiTest_Agent:process getattr;
')

